{
  "name": "webapp",
  "version": "0.0.1",
  "scripts": {
    "main": "node index.js",
    "start": "node tools/dev-server.js",
    "dev": "node tools/dev-server.js",
    "dev-daily": "node tools/dev-server.js --dev-daily",
    "dev-pre": "node tools/dev-server.js --pre",
    "dev-prod": "node tools/dev-server.js --prod",
    "prebuild:js": "npm run lint",
    "lint": "eshint **",
    "deploy": "",
    "prod": "rimraf build && webpack --prod --progress --hide-modules --config tools/webpack.prod.conf.js",
    "pre": "rimraf build && mkdir build && webpack --pre --progress --hide-modules --config tools/webpack.prod.conf.js",
    "daily": "rimraf build && mkdir build && webpack --daily --progress --hide-modules --config tools/webpack.prod.conf.js"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.9.0",
    "babel-loader": "^6.0.0",
    "browser-sync": "^2.11.1",
    "browser-sync-webpack-plugin": "^1.0.1",
    "connect-history-api-fallback": "^1.1.0",
    "css-loader": "^0.23.0",
    "eslint": "^2.1.0",
    "eslint-loader": "^1.2.0",
    "express": "^4.13.3",
    "express-favicon": "^1.0.1",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.4",
    "function-bind": "^1.0.2",
    "git-rev-sync": "^1.5.0",
    "html-webpack-plugin": "^2.12.0",
    "inject-loader": "^2.0.1",
    "less": "^2.6.0",
    "less-loader": "^2.2.2",
    "lodash": "^4.13.1",
    "postcss-loader": "^0.9.1",
    "rimraf": "^2.5.2",
    "standard": "^6.0.8",
    "string-replace-webpack-plugin": "0.0.3",
    "style-loader": "^0.13.0",
    "underscore-template-loader": "^0.7.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.6.0"
  }
}
